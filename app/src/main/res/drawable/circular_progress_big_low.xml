    android:fromDegrees="270"
    android:toDegrees="270"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <shape
        android:shape="ring"
        android:thicknessRatio="7"
        android:useLevel="true">

        <gradient
            android:startColor="#D5432F"
            android:endColor="#D34D24"
            android:angle="90"/>
    </shape>

</rotate>